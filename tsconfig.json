{
    "compilerOptions": {
        "target": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "useUnknownInCatchVariables": false
    },
    "include": ["/**/*"],
    "exclude": ["node_modules"]
}
